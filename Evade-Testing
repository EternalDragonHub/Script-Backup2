function GetEmotesName()
    local EmoteNames = {}
    for _, Thing in pairs(game:GetService("ReplicatedStorage").Items.Emotes:GetChildren()) do
        if Thing:IsA("LocalScript") or Thing:IsA("ModuleScript") then
            table.insert(EmoteNames, Thing.Name)
        end
    end
    return EmoteNames
end

