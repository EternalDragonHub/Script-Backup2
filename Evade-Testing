    Toggle:OnChanged(function(State)
        AutoWhistle = State
      while task.wait(1) and AutoWhistle do
			autowistlefunction()
		end
    end)
    
function nodamagepart()
   local Map = game.Workspace.Game.Map
   
   for i, v in pairs(Map:GetDescendants()) do
     if (v.Name == "Water" or v.Name == "Lava") or v:IsA("BasePart") and v:FindFirstChildWhichIsA("TouchTransmitter") then
          v.CanTouch = false
       end
   end
end

